import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for ( int i=0;i<n;i++){
            for ( int j=0;j<=i;j++){
                char ch=(char) ('A' + i + j);
                System.out.print(ch + " ");
            }
            System.out.println();
        }
    }
}
